	namespace "urn:vyatta.com:mgmt:vyatta-ippf-deviations-siad:1";
	prefix vyatta-ippf-deviations-siad-v1;

	import vyatta-security-v1 {
		prefix security;
	}

	import vyatta-ippf-v1 {
		prefix ippf;
	}

	organization "AT&T Inc.";
	contact
		"AT&T
		 Postal: 208 S. Akard Street
		         Dallas, TX 75202, USA
		 Web: www.att.com";

